, password }),\n    })\n      .then((response) => {\n        if (response.ok) {\n          // Redirect to the dashboard page if login is successful\n          window.location.href = '/dashboard';\n        } else {\n          throw new Error('Login failed');\n        }\n      })\n      .catch((error) => {\n        console.error(error);\n        // Display an error message to the user\n        alert('Login failed. Please try again.');\n      });\n  };\n\n  return (\n    <div className={LoginStyle.container}>\n      <div className={LoginStyle.formContainer}>\n        <form className={LoginStyle.loginForm} onSubmit={handleLogin}>\n          <h1 className={LoginStyle.h1}>Login</h1>\n          <label className={LoginStyle.label} htmlFor=\"email\">Email:</label>\n          <input className={LoginStyle.input}\n            type=\"email\"\n            id=\"email\"\n            name=\"email\"\n            value={email}\n            onChange={(event) => setEmail(event.target.value)}\n            required\n          />\n          <label htmlFor=\"password\">Password:</label>\n          <input className={LoginStyle.input}\n            type=\"password\"\n            id=\"password\"\n            name=\"password\"\n            value={password}\n            onChange={(event) => setPassword(event.target.value)}\n            required\n          />\n          <button className={LoginStyle.button} type=\"submit\">Login</button>\n          <p className={LoginStyle.p}>\n            Don't have an account? <a className={LoginStyle.a} href=\"#\">Register here</a>.\n          </p>\n        </form>\n        <form className={LoginStyle.registrationForm}>\n          <h1 className={LoginStyle.h1}>Register</h1>\n          <label className={LoginStyle.label} htmlFor=\"name\">Name:</label>\n          <input className={LoginStyle.input} type=\"text\" id=\"name\" name=\"name\" required />\n          <label className={LoginStyle.label} htmlFor=\"email\">Email:</label>\n          <input className={LoginStyle.input} type=\"email\" id=\"email\" name=\"email\" required />\n          <label className={LoginStyle.label} htmlFor=\"password\">Password:</label>\n          <input className={LoginStyle.input} type=\"password\" id=\"password\" name=\"password\" required />\n          <label className={LoginStyle.label} htmlFor=\"confirm-password\">Confirm Password:</label>\n          <input className={LoginStyle.input} type=\"password\" id=\"confirm-password\" name=\"confirm-password\" required />\n          <button className={LoginStyle.button} type=\"submit\">Register</button>\n          <p className={LoginStyle.p}>\n            Already have an account? <a className={LoginStyle.a} href=\"#\">Login here</a>.\n          </p>\n        </form>\n      </div>\n    </div>\n  );\n}\n\nexport default Login;\n",{"ruleId":"60","severity":1,"message":"61","line":2,"column":8,"nodeType":"62","messageId":"63","endLine":2,"endColumn":18},{"ruleId":"64","severity":1,"message":"65","line":10,"column":17,"nodeType":"66","endLine":10,"endColumn":48},{"ruleId":"64","severity":1,"message":"65","line":11,"column":17,"nodeType":"66","endLine":11,"endColumn":30},{"ruleId":"64","severity":1,"message":"65","line":12,"column":17,"nodeType":"66","endLine":12,"endColumn":29},{"ruleId":"64","severity":1,"message":"65","line":13,"column":17,"nodeType":"66","endLine":13,"endColumn":29},{"ruleId":"64","severity":1,"message":"65","line":14,"column":17,"nodeType":"66","endLine":14,"endColumn":29},{"ruleId":"64","severity":1,"message":"65","line":9,"column":40,"nodeType":"66","endLine":9,"endColumn":71},{"ruleId":"64","severity":1,"message":"65","line":10,"column":40,"nodeType":"66","endLine":10,"endColumn":52},{"ruleId":"64","severity":1,"message":"65","line":11,"column":40,"nodeType":"66","endLine":11,"endColumn":52},{"ruleId":"64","severity":1,"message":"65","line":12,"column":40,"nodeType":"66","endLine":12,"endColumn":52},{"ruleId":"64","severity":1,"message":"65","line":13,"column":40,"nodeType":"66","e